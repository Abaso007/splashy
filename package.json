{
  "name": "splashy",
  "description": "Given an image, extract predominant & palette colors",
  "homepage": "https://documentup.com/Kikobeats/splashy",
  "version": "2.0.4",
  "main": "index.js",
  "author": {
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikobeats/splashy.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/splashy/issues"
  },
  "keywords": [
    "canvas",
    "color",
    "colour",
    "dominant",
    "extract",
    "extraction",
    "file",
    "image",
    "main",
    "palette",
    "photo",
    "picture",
    "url"
  ],
  "dependencies": {
    "async": "~2.6.0",
    "node-vibrant": "~3.0.0",
    "pump": "~3.0.0",
    "run-waterfall": "~1.1.6",
    "simple-get": "~3.0.2",
    "tempy": "~0.2.1"
  },
  "devDependencies": {
    "coveralls": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "should": "latest",
    "standard": "11",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon --exec \"npm start\" -e \"js\"",
    "lint": "standard-markdown README.md && standard",
    "precommit": "lint-staged",
    "prelint": "npm run pretty",
    "pretest": "npm run lint",
    "pretty": "prettier-standard {core,test,bin}/**/*.js",
    "start": "node index.js",
    "test": "nyc mocha"
  },
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
